{
  "expo": {
    "name": "MindDash",
    "slug": "MindDash",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/loading/icon_mind.png",
    "userInterfaceStyle": "off",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/loading/loading-mind.png",
      "resizeMode": "contain",
      "backgroundColor": "#2d5016"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loading/loading-mind.png",
        "backgroundColor": "#2d5016"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/loading/icon_mind.png"
    }
  }
}
